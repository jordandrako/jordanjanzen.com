{
  "name": "jordanjanzen.com",
  "homepage": "https://jordanjanzen.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_check": "node scripts/check.js",
    "prestart": "yarn run check",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "yarn run check -p && rm -rf build && cross-env REACT_APP_DATABASE=production yarn build",
    "prestaging": "yarn run check && rm -rf build && yarn build",
    "deploy": "firebase deploy -P production",
    "staging": "firebase deploy -P staging",
    "check:env": "yarn _check --env",
    "check:branch": "yarn _check --branch",
    "check": "yarn _check --all",
    "lint:ts": "tslint -p tsconfig.json --fix",
    "lint:styles": "node_modules/.bin/stylelint --fix src",
    "lint": "yarn lint:ts && yarn lint:styles"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "date-fns": "^1.29.0",
    "dynamic-polyfill": "^1.1.0",
    "firebase": "^5.5.5",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "polished": "^1.9.3",
    "re-base": "^3.2.3",
    "react": "^16.6.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.0",
    "react-dropzone": "^4.3.0",
    "react-ga": "^2.5.3",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-transition-group": "^2.5.0",
    "sha1": "^1.1.1",
    "styled-components": "^3.4.10",
    "styled-normalize": "^4.0.0",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "@types/re-base": "^3.2.0",
    "@types/react": "^16.4.18",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-loadable": "^5.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^2.0.14",
    "@types/sha1": "^1.1.1",
    "@types/superagent": "^3.8.4",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "git-branch": "^2.0.1",
    "prettier": "^1.14.3",
    "prettier-tslint": "^0.4.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  },
  "peerDependencies": {
    "firebase-tools": "^6.0.0"
  }
}
