{
  "name": "jordanjanzen.com",
  "homepage": "https://jordanjanzen.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "yarn checkenv && scripts/branch.sh",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "checkenv": "node scripts/check-env.js",
    "branchcheck": "git-branch-is master",
    "predeploy": "yarn branchcheck && yarn checkenv && scripts/branch.sh && rm -rf build && cross-env REACT_APP_DATABASE=production yarn build",
    "prestaging": "yarn checkenv && rm -rf build && scripts/branch.sh && yarn build",
    "deploy": "firebase deploy -P production",
    "staging": "firebase deploy -P staging",
    "lint:ts": "tslint -p tsconfig.json --fix",
    "lint:styles": "node_modules/.bin/stylelint --fix src",
    "lint": "yarn lint:ts && yarn lint:styles"
  },
  "dependencies": {
    "check-env": "^1.3.0",
    "dotenv": "^5.0.1",
    "dynamic-polyfill": "^1.1.0",
    "firebase": "^5.0.4",
    "history": "^4.7.2",
    "moment": "^2.22.1",
    "polished": "^1.9.2",
    "re-base": "^3.2.2",
    "react": "^16.3.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.9",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.2",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1",
    "react-transition-group": "^2.3.1",
    "sha1": "^1.1.1",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/re-base": "^3.2.0",
    "@types/react": "^16.3.13",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.0.5",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-loadable": "^5.3.4",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-transition-group": "^2.0.9",
    "@types/sha1": "^1.1.1",
    "@types/superagent": "^3.5.8",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "git-branch-is": "^0.1.0",
    "prettier": "^1.12.1",
    "prettier-tslint": "^0.4.0",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "firebase-tools": "^3.18.4"
  }
}
