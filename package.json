{
  "name": "jordanjanzen.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "cross-env REACT_APP_DATABASE=production npm run build",
    "prestaging": "npm run build",
    "deploy": "aws s3 sync build/ s3://www.jordanjanzen.com --cache-control max-age=172800",
    "staging": "aws s3 sync build/ s3://staging.jordanjanzen.com --cache-control max-age=180",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E1SASUQHSLUTW3 --paths /index.html",
    "poststaging": "aws cloudfront create-invalidation --distribution-id E2R8NA2ILW4G96 --paths /index.html"
  },
  "dependencies": {
    "firebase": "^4.13.1",
    "moment": "^2.22.1",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "re-base": "^3.2.2",
    "react": "^16.3.2",
    "react-autosize-textarea": "^3.0.3",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.2.9",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1",
    "react-transition-group": "^2.3.1",
    "sha1": "^1.1.1",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-eslint": "^7.2.3",
    "cross-env": "^5.1.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.12.1",
    "typescript": "^2.8.3"
  }
}
